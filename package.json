{
  "name": "@cpdevtools/template-github-action",
  "repository": "https://github.com/cpdevtools/template-github-action",
  "version": "0.0.0-dev.0",
  "scripts": {
    "prepare": "run-s prepare:*",
    "prepare:build": "tsc -p .schematic/tsconfig.json",
    "prepare:initialize": "pnpm run schematics && pnpm i && git add --all && git commit -a -m \"template initialized\"",
    "schematics": "schematics .:initialize --debug=false --template-path=.template"
  },
  "schematics": ".schematic/collection.json",
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "15.0.4",
    "@angular-devkit/schematics-cli": "15.0.4",
    "@cpdevtools/lib-node-utilities": "0.4.49",
    "@types/node": "18.11.17",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "npm-run-all2": "6.0.4",
    "prettier": "2.8.1",
    "type-fest": "3.4.0",
    "typescript": "4.9.4"
  },
  "fromTemplate": [
    "@cpdevtools/template-generate-template@0.0.1-dev.1"
  ]
}