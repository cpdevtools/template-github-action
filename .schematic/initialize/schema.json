{
  "$schema": "http://json-schema.org/schema",
  "$id": "Options",
  "title": "Options",
  "type": "object",
  "properties": {
    "templatePath": {
      "description": "The path to the source template",
      "type": "string",
      "default": "template"
    },
    "deleteSchematic": {
      "description": "Delete the .schematic folder after init",
      "type": "boolean",
      "default": true
    }
  }
}
